<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" >
		<shape>
			<solid
				android:color="#E8E8E8" />
			<stroke
				android:width="1dp"
				android:color="#E8E8E8" />
			<corners
				android:radius="30dp" />
			<padding
				android:left="20dp"
				android:top="20dp"
				android:right="20dp"
				android:bottom="20dp" />
		</shape>
	</item>
	<item>
		<shape>
			<solid
				android:color="#D2D2D2" />
			<stroke
				android:width="1dp"
				android:color="#D2D2D2" />
			<corners
				android:radius="30dp" />
			<padding
				android:left="20dp"
				android:top="20dp"
				android:right="20dp"
				android:bottom="20dp" />
		</shape>
	</item>
</selector>
